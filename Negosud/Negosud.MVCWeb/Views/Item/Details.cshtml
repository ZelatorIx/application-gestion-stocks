@using Negosud.MVCWeb.Types;
@{
    ViewData["Title"] = "Articles";
    Item item = Model;
}

<a asp-action="Index">Retour</a>

<div>Nom : @item.Name</div>
<div>Description : @item.Description</div>
<div>Famille : @item.Family.Name</div>
<div>Année : @item.YearItem</div>
<div>Prix : @(item.SellingPriceBT * item.Vat)&euro;</div>

<a>Ajouter au panier</a>